<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Pressed -->
    <item android:state_pressed="true" android:color="#161616"/>
    <!-- Focused (keyboard / TV) -->
    <item android:state_focused="true" android:color="#141414"/>
    <!-- Disabled -->
    <item android:state_enabled="false" android:color="#33000000"/>
    <!-- Default -->
    <item android:color="#000000"/>
</selector>